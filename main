import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("Üssü Alınacak Sayı:");
        int n = scan.nextInt();
        System.out.println("Üs Olacak Sayı:");
        int k = scan.nextInt();
   
    int total=1;
    for (int i=1;i<=k;i++){
        total*=n;
            }
        System.out.println("CEVAP:"+total);
    }
}
